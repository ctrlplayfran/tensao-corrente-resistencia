#calcular o resistor ideal
miliampere = float(input("Entre com o valor da corrente elÃ©trica em miliamperes: "))
tensao_bateria = float(input("Digite o valor da tensÃ£o da bateria (V)"))
tensao_led_aguenta = float(input("Entre com om valor da tensÃ£o que o led suporta:"))
ampere = miliampere/1000
resistencia = tensao_bateria - tensao_led_aguenta / ampere
resistencia = abs(resistencia)
if resistencia >= 40 and resistencia <=240:
    print("Resistencia pode ser usada")
